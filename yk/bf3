#!yasl

{
	print "\n" print
} puts

{
	""
	1 @(
		read ? (
			+
			1
		) : (
			!
			0
		)
	)
	"_" +
} read_out

{
	"\0         \n                      !\"#$%&'()*+,-./0123456789:;<=>?@ABCDEFGHIJKLMNOPQRSTUVWXYZ[\\]^_`abcdefghijklmnopqrstuvwxyz{|}~                                                                                                                                 "
	2 ^
	]#[
	print
}	putchar

{
	1 + ^
}	popup_item

{
	1 + !^
}	pushdown_item

{ "A" - 65 + } char_to_i

// -------------------------------------------------------------

// deploy stacks

// 1: [code: string]
(! ! =:1 !)
1:("_" +):1

// 0: [ptr: int]
1

// 2: [pc int, ...]
0:2

// 9: [memory: int[30000]]
9:(30000 #(0)):9

{
	2:=:2
	// 0: [ptr: int]
	// 9: [memory: int[30000]]
	// 2: [pc_in_local: int, pc_at_start: int, ...]
	1 @(
		2:=:1 1:%]#[ 1:!

		// 0: [op, ...]
		// (= "op is: \"" 2 ^ + "\"" + puts )
		(= "_" == ) ? (
			// ("program end" puts)
			0
		) : (
		(= ">" == ) ? (
			// "<": increment ptr
			// ("increment ptr" puts)
			! 1 +
			1
		) : (
		(= "<" == ) ? (
			// ">": decrement ptr
			// ("decrement ptr" puts)
			! 1 -
			1
		) : (
		(= "+" == ) ? (
			// ("increment *ptr" puts)
			!
			9:(
				(0:= popup_item)    // front item
				1 +                 // increment item
				(0:= pushdown_item) // back item
			):9
			1
		) : (
		(= "-" == ) ? (
			// ("decrement *ptr" puts)
			!
			9:(
				(0:= popup_item)    // front item
				1 -                 // decrement item
				(0:= pushdown_item) // back item
			):9
			1
		) : (
		(= "." == ) ? (
			// ("output *ptr" puts)
			!
			9:(
				(0:= popup_item)    // front item
				(= putchar)
				(0:= pushdown_item) // back item
			):9
			1
		) : (
		(= "," == ) ? (
			// ("read a byte and set into *ptr" puts)
			!
			9:(
				(0:= popup_item)    // front item
				! (
					read
					!
					0 ]#[
				)
				char_to_i  // read char
				(0:= pushdown_item) // back item
			):9
			1
		) : (
			! 1
		)))))))

		? (
			// "increment pc" puts
			2:(1 +):2
			1
		) : 0
	)
} interpret

1:(= puts):1
0:interpret:0
